<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:text="Posl. dog."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="33.0dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/textView1" />
    <Spinner
        android:spinnerMode="dialog"
        android:layout_width="match_parent"
        android:layout_height="49.5dp"
        android:id="@+id/cbDocType" />
    <TextView
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="38.0dp"
        android:id="@+id/textView2"
        android:text="Skladišče" />
    <Spinner
        android:spinnerMode="dialog"
        android:layout_width="match_parent"
        android:layout_height="53.0dp"
        android:id="@+id/cbWarehouse" />
    <TextView
        android:text="...."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lbExtra" />
    <Spinner
        android:layout_width="match_parent"
        android:spinnerMode="dialog"
        android:layout_height="69.5dp"
        android:id="@+id/cbExtra" />
    <TextView
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="108.5dp"
        android:id="@+id/textView4"
        android:layout_marginBottom="0.0dp" />
    <Button
        android:background="@drawable/nicelookingbutton"
        android:layout_width="match_parent"
        android:layout_height="49.0dp"
        android:id="@+id/btnOrder"
        android:text="Potrdi - F2" />
    <Button
        android:background="@drawable/nicelookingbutton"
        android:text="Brez naročila - F3"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/btnOrderMode"
        android:layout_marginBottom="2.0dp" />
    <Button
        android:background="@drawable/nicelookingbutton"
        android:text="izhod - f9"
        android:layout_width="match_parent"
        android:layout_height="46.5dp"
        android:id="@+id/btnLogout" />

</LinearLayout>
