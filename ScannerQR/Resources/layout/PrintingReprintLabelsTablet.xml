<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/test"
        android:layout_toRightOf="@id/app"
        android:layout_below="@id/text"
        />


    
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/test"
      
        android:layout_toRightOf="@id/app"
        android:id="@+id/listView1"
        />
   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:layout_toRightOf="@id/app"
      
        android:weightSum="5"
        android:id="@+id/text"
        android:paddingTop="5px">

        <TextView
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident"
            android:layout_marginRight="28.0dp"
            android:id="@+id/ident"
            android:layout_width="match_parent" />

        <TextView
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Naziv"
            android:layout_width="match_parent"
            android:layout_marginRight="28.0dp"
            android:id="@+id/name" />

        <TextView
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Naročeno"
            android:layout_width="match_parent"
            android:layout_marginRight="28.0dp"
            android:id="@+id/quantity" />
        <TextView
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Prejeto"
            android:layout_width="match_parent"
            android:layout_marginRight="28.0dp"
            android:id="@+id/position" />


        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:textStyle="bold"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Odprto"
            android:layout_marginRight="28.0dp"
            android:id="@+id/serial" />



     

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_widthPercent="30%"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="@+id/app"
        tools:context=".LinearLayout"
        android:foregroundGravity="center">
  <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Tiskanje etiket - ponovno tiskanje" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident"
            android:layout_marginRight="62.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbIdent" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Naziv"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="57.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbTitle" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="SSCC koda"
            android:layout_marginRight="20.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbSSCC" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Serijska št."
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:layout_marginRight="20.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbSerialNum" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Skladišče"
            android:layout_marginRight="48.0dp" />

        <Spinner
            android:spinnerMode="dialog"
            android:layout_width="match_parent"
            android:layout_height="42.5dp"
            android:id="@+id/cbWarehouse"
            android:layout_marginTop="24.5dp" />



    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Lokacija"
            android:layout_marginRight="33.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbLocation" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Količina"
            android:layout_marginRight="33.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbQty" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="57.0dp"
        android:orientation="horizontal"
        android:paddingTop="5px">



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Subjekt"
            android:layout_marginRight="55.0dp" />
        <Spinner
            android:spinnerMode="dialog"
            android:layout_width="match_parent"
            android:layout_height="25.0dp"
            android:id="@+id/cbSubject"
            android:layout_marginTop="24.5dp" />


    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="39.0dp"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:layout_marginRight="55.0dp" />
  <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:paddingTop="5px">

        <Button
            android:textSize="16px"
            android:background="@drawable/nicelookingbutton"
            android:text="Tiskaj-f3"
            android:drawableLeft="@drawable/printer"
            android:paddingLeft="10px"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:layout_weight="1"
            android:id="@id/btPrint" />

        <Button
           android:textSize="16px"
            android:background="@drawable/nicelookingbutton"
            android:text="Izhod-f8"
            android:drawableLeft="@drawable/logout"
            android:paddingLeft="10px"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:id="@+id/button2" />


    </LinearLayout>

    </LinearLayout>









</android.support.percent.PercentRelativeLayout>