<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    tools:context=".LinearLayout"
    android:foregroundGravity="center">
    <TextView

        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Tiskanje - vhodna kontrola"
        android:id="@+id/lbInfo" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView

            android:textStyle="bold"
            android:layout_width="126.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Datum" />

        <EditText

            android:textStyle="bold"
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/dtDate" />

        <Button
            android:text="Izberi"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/dateChoice" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:textSize="16px"
            android:textStyle="bold"
            android:layout_width="125.0dp"
            android:layout_height="wrap_content"
            android:text="Uporabnik"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1" />

        <EditText
            android:textStyle="bold"
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbUser" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Vhodna kontrola"
            android:layout_marginBottom="14.5dp"
            android:id="@+id/lbInfoinput" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:textStyle="bold"
            android:layout_width="122.5dp"
            android:layout_height="wrap_content"
            android:text="Prevzem"
            android:paddingTop="5px"
            android:paddingLeft="10px"/>

        <EditText
            android:textStyle="bold"
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbTakeOver" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView

            android:textStyle="bold"
            android:layout_width="120.5dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Dobavitelj" />

        <EditText

            android:textStyle="bold"
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbSupplier" />



    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView

            android:textStyle="bold"
            android:layout_width="119.5dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Datum prevzema" />

        <EditText
            android:textStyle="bold"
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbTakeOverDate" />

    </LinearLayout>
    <TextView
        android:layout_width="119.5dp"
        android:layout_height="95.0dp"
        android:paddingLeft="10px"
        android:paddingTop="5px"/>
 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:paddingTop="5px">

        <Button
            android:textSize="16px"
            android:background="@drawable/nicelookingbutton"
            android:text="Naslednja -F2"
            android:drawableLeft="@drawable/next"
            android:paddingLeft="10px"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:id="@+id/btNext" />

        <Button
            android:textSize="16px"
            android:background="@drawable/nicelookingbutton"
            android:text="Tiskaj-F3"
            android:drawableLeft="@drawable/printer"
            android:paddingLeft="10px"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:id="@+id/btPrint" />


    </LinearLayout>
    <Button
        android:background="@drawable/nicelookingbutton"
        android:text="Izhod - F8"
        android:layout_width="match_parent"
        android:layout_height="70.0dp"
        android:textSize="16px"
        android:drawableLeft="@drawable/logout"
        android:paddingLeft="10px"
        android:id="@+id/button3"
        android:layout_marginTop="0.0dp" />


</LinearLayout>