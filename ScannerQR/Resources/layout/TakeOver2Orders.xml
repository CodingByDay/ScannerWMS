<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:textStyle="bold"
            android:layout_width="120.0dp"
            android:layout_height="wrap_content"
            android:text="Ident"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:textStyle="bold"
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbIdent" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:textStyle="bold"
            android:layout_width="120.0dp"
            android:layout_height="wrap_content"
            android:text="Naziv"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView21"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:textStyle="bold"
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbNaziv" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Količina prevzema"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView31"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:textStyle="bold"
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbKolicinaPrevzema" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:textStyle="bold"
            android:layout_width="115.5dp"
            android:layout_height="wrap_content"
            android:text="Naročilo"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/lblOrder"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:textStyle="bold"
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbNarocilo" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:layout_width="116.0dp"
            android:layout_height="wrap_content"
            android:text="Kupec"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView12"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="39.5dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbKupec" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:layout_width="113.5dp"
            android:layout_height="wrap_content"
            android:text="Datum dostave"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView13"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="32.0dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbDatumDostave" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="40.5dp"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kol. odp. /"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView132"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbKolicinaOdprta" />
        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbKolicinaPrevzetaDoSedaj" />
        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbKolicinaPrevzetaNova" />

    </LinearLayout>
  <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:paddingTop="5dp">

        <Button
            android:textSize="20dp"
            android:background="@drawable/nicelookingbutton"
            android:text="nasled. - f2"
            android:drawableLeft="@drawable/next"
            android:paddingLeft="10dp"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="70.0dp"
            android:id="@+id/button1" />

        <Button
            android:layout_weight="1"
            android:textSize="20dp"
            android:background="@drawable/nicelookingbutton"
            android:text="prejšn. - f3"
            android:drawableLeft="@drawable/left"
            android:paddingLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:id="@+id/button2" />


    </LinearLayout>





  <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:paddingTop="5dp">

        <Button
            android:layout_weight="1"
            android:background="@drawable/nicelookingbutton"
            android:text="natisni - f4"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:textSize="20dp"
            android:paddingLeft="10dp"
            android:drawableLeft="@drawable/sticker"
            android:id="@+id/button3" />

        <Button
            android:layout_weight="1"
            android:background="@drawable/nicelookingbutton"
            android:text="tiskaj - f5"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:drawableLeft="@drawable/printer"
            android:textSize="20dp"
            android:paddingLeft="10dp"
            android:id="@+id/button4" />


    </LinearLayout>


 <LinearLayout
        android:weightSum="2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <Button
            android:layout_weight="1"
            android:background="@drawable/nicelookingbutton"
            android:text="n. ident - f6"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:textSize="20dp"
            android:paddingLeft="10dp"
            android:drawableLeft="@drawable/ident"
            android:id="@+id/button5" />

        <Button
            android:layout_weight="1"
            android:background="@drawable/nicelookingbutton"
            android:text="Izhod - F8"
            android:layout_width="match_parent"
           android:layout_height="70.0dp"
           android:textSize="20dp"
            android:drawableLeft="@drawable/logout"
            android:paddingLeft="10dp"
            android:id="@+id/button6" />


    </LinearLayout>
</LinearLayout>
