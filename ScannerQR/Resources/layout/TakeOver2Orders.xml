<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="120.0dp"
            android:layout_height="wrap_content"
            android:text="Ident"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbIdent" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="120.0dp"
            android:layout_height="wrap_content"
            android:text="Naziv"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbNaziv" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Količina prevzema"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbKolicinaPrevzema" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="115.5dp"
            android:layout_height="wrap_content"
            android:text="Naročilo"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/lblOrder"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbNarocilo" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="116.0dp"
            android:layout_height="wrap_content"
            android:text="Kupec"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbKupec" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="113.5dp"
            android:layout_height="wrap_content"
            android:text="Datum dostave"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbDatumDostave" />

    </LinearLayout><LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kol. odp. /"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="41.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbKolicinaOdprta" />
        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbKolicinaPrevzetaDoSedaj" />
        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbKolicinaPrevzetaNova" />

    </LinearLayout>
    <Button
        android:text="naslednje naročilo-f2"
        android:layout_width="match_parent"
        android:layout_height="47.5dp"
        android:id="@+id/button1" />
    <Button
        android:text="prejšnje naročilo-f3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button2" />
    <Button
        android:text="natisni etiketo-f4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button3" />
    <Button
        android:text="tiskaj za količino-f5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button4" />
    <Button
        android:text="naslednji ident-f6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button5" />
</LinearLayout>
