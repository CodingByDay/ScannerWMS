<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">






    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/test"
      
        android:layout_toRightOf="@id/app"
        android:id="@+id/listView1"
        />
                                               
   


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/app"
        android:layout_above="@id/list"
        android:weightSum="5"
        android:id="@+id/text"
        android:paddingTop="5px">

        <TextView
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident"
            android:layout_marginRight="28.0dp"
            android:id="@+id/ident"
            android:layout_width="match_parent" />

        <TextView
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Naziv"
            android:layout_width="match_parent"
            android:layout_marginRight="28.0dp"
            android:id="@+id/name" />




        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:textStyle="bold"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Naro훾eno"
            android:layout_marginRight="28.0dp"
            android:id="@+id/ordered" />


               <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:textStyle="bold"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Prejeto"
            android:layout_marginRight="28.0dp"
            android:id="@+id/received" />


               <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:textStyle="bold"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Odprto"
            android:layout_marginRight="28.0dp"
            android:id="@+id/open" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_widthPercent="30%"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="@+id/app"
        tools:context=".LinearLayout"
        android:foregroundGravity="center">

         <TextView
        android:singleLine="true"
        android:layout_width="wrap_content"
        android:layout_height="27.0dp"
        android:layout_gravity="center"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Prevzem od dobavitelja" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:singleLine="true"
            android:layout_width="77.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident"
            android:layout_marginRight="39.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbIdent" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:singleLine="true"
            android:layout_width="115.5dp"
            android:layout_height="wrap_content"
            android:text="Naziv"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbNaziv" />

    </LinearLayout>
    <TextView
        android:singleLine="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Naro훾ilo"
        android:id="@+id/lbOrderInfo" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:singleLine="true"
            android:layout_width="115.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Naro훾ilo" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbOrder" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="114.5dp"
            android:layout_height="wrap_content"
            android:text="Dobavitelj"
            android:paddingTop="5px"
            android:paddingLeft="10px"/>

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbConsignee" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="115.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Datum dobave" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbDeliveryDeadline" />



    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="113.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Koli훾ina" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="35.5dp"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbQty"/>

    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="10px"
        android:paddingTop="5px"/>
    <TextView
        android:layout_width="113.0dp"
        android:layout_height="57.5dp"
        android:paddingLeft="10px"
        android:paddingTop="5px"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

            <Button
                android:background="@drawable/nicelookingbutton"
                android:text="Nasl. nar-f2"
                android:layout_width="184.5dp"
                android:layout_height="70.0dp"
                android:textSize="16px"
                android:paddingLeft="10px"
                android:drawableLeft="@drawable/next"
                android:id="@+id/btNext" />

        <Button
            android:background="@drawable/nicelookingbutton"
            android:text="potrdi-f3"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:textSize="16px"
            android:drawableLeft="@drawable/confirmation"
            android:paddingLeft="10px"
            android:id="@+id/btConfirm"
            android:layout_marginTop="0.0dp"
            android:layout_marginBottom="0.0dp" />


    </LinearLayout>
     <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

            <Button
                android:background="@drawable/nicelookingbutton"
                android:text="pregled -f4"
                android:layout_width="184.5dp"
                android:layout_height="70.0dp"
                android:textSize="16px"
                android:paddingLeft="10px"
                android:drawableLeft="@drawable/focus"
                android:id="@+id/button4" />

        <Button
            android:background="@drawable/nicelookingbutton"
            android:text="izhod-f9"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:textSize="16px"
            android:drawableLeft="@drawable/logout"
            android:paddingLeft="10px"
            android:id="@+id/button5"
            android:layout_marginTop="0.0dp"
            android:layout_marginBottom="0.0dp" />


    </LinearLayout>



   




    </LinearLayout>



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/test"
        android:layout_toRightOf="@id/app"
        android:layout_below="@id/text"
        android:text="                               *Pritiskom na en ident se bo otprlo okno."
        />


   
  



</android.support.percent.PercentRelativeLayout>