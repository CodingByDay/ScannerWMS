<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    tools:context=".LinearLayout"
    android:foregroundGravity="center">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Prevzem iz proizvodnje" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="95.5dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbIdent" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="92.5dp"
            android:layout_height="wrap_content"
            android:text="SSCC koda"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbSSCC" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="91.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Serijska št." />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbSerialNum" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="89.5dp"
            android:layout_height="wrap_content"
            android:text="Lokacija"
            android:paddingTop="5px"
            android:paddingLeft="10px"/>

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbLocation" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="88.5dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Količina"
            android:id="@+id/lbQty" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbPacking" />

        <Button
            android:text="-(F1)"
            android:layout_width="74.0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button1" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="86.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Št. enot" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbUnits" />


    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="107.0dp"
        android:paddingLeft="10px"
        android:paddingTop="5px"/>
  <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <Button
            android:background="@drawable/nicelookingbutton"
            android:text="Vnesi serijsko št.-f2"
            android:layout_width="184.5dp"
            android:layout_height="59.5dp"
            android:textSize="16px"
            android:paddingLeft="10px"
            android:drawableLeft="@drawable/newitem"
            android:id="@+id/btSaveOrUpdate" />

        <Button
            android:background="@drawable/nicelookingbutton"
            android:text="pregled vnesenih pozicij-f3"
            android:layout_width="match_parent"
            android:layout_height="56.5dp"
            android:textSize="16px"
            android:drawableLeft="@drawable/focus"
            android:paddingLeft="10px"
            android:id="@+id/button3"
            android:layout_marginTop="0.0dp"
            android:layout_marginBottom="0.0dp" />


    </LinearLayout>
     <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <Button
            android:background="@drawable/nicelookingbutton"
            android:text="Zaljuči prevzem-f4"
            android:layout_width="184.5dp"
            android:layout_height="59.5dp"
            android:textSize="16px"
            android:paddingLeft="10px"
            android:drawableLeft="@drawable/shipped"
            android:id="@+id/button4" />

        <Button
            android:background="@drawable/nicelookingbutton"
            android:text="izhod-f8"
            android:layout_width="match_parent"
            android:layout_height="56.5dp"
            android:textSize="16px"
            android:drawableLeft="@drawable/logout"
            android:paddingLeft="10px"
            android:id="@+id/button5"
            android:layout_marginTop="0.0dp"
            android:layout_marginBottom="0.0dp" />


    </LinearLayout>


</LinearLayout>