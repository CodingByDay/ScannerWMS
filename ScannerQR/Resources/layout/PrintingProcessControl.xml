<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    tools:context=".LinearLayout"
    android:foregroundGravity="center">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Tiskanje - procesna kontrola"
        android:id="@+id/lbInfo" />
        <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="138.0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Datum" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/dtDate"
            
            />

        <Button
            android:text="Izberi"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/dateChoice" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Uporabnik"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="58.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbUser" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Procesna kontrola"
            android:layout_marginBottom="14.5dp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="88.0dp"
            android:layout_height="wrap_content"
            android:text="Izmena"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:layout_marginRight="36.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbShift" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Delavec"
            android:layout_marginRight="68.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbWorker" />



    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="SSCC koda"
            android:layout_marginRight="48.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbSSCC" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident"
            android:layout_marginRight="84.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbIdent" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Naziv"
            android:layout_marginRight="79.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbTitle" />

    </LinearLayout>
    <TextView
        android:layout_width="138.0dp"
        android:layout_height="28.0dp"
        android:paddingLeft="10px"
        android:paddingTop="5px"/>
    <Button
        android:background="@drawable/nicelookingbutton"
        android:text="Naslednja kontrola - F2"
        android:layout_width="match_parent"
        android:layout_height="41.0dp"
        android:id="@+id/btNext"
        android:layout_marginBottom="0.0dp" />
    <Button
        android:background="@drawable/nicelookingbutton"
        android:text="Tiskaj - F3"
        android:layout_width="match_parent"
        android:layout_height="42.0dp"
        android:id="@+id/btPrint"
        android:layout_marginBottom="1.0dp"
        android:layout_marginTop="0.0dp" />
    <Button
        android:background="@drawable/nicelookingbutton"
        android:text="Izhod - F9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:layout_marginTop="0.0dp" />


</LinearLayout>