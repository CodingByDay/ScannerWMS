<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    tools:context=".LinearLayout"
    android:foregroundGravity="center">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:text="Tiskanje etiket - ponovno tiskanje" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Ident"
            android:layout_marginRight="62.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40dp"
            android:paddingTop="5dp"
            android:id="@+id/tbIdent" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Naziv"
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView1"
            android:layout_marginRight="57.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbTitle" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="SSCC koda"
            android:layout_marginRight="20.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40dp"
            android:paddingTop="5dp"
            android:id="@+id/tbSSCC" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Serijska št."
            android:paddingTop="5dp"
            android:paddingLeft="10dp"
            android:layout_marginRight="20.0dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="39.5dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_weight="0.50"
            android:paddingTop="5dp"
            android:id="@+id/tbSerialNum" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="64.5dp"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Skladišče"
            android:layout_marginRight="48.0dp" />

        <Spinner
            android:spinnerMode="dialog"
            android:layout_width="match_parent"
            android:layout_height="38.0dp"
            android:id="@+id/cbWarehouse"
            android:layout_marginTop="24.5dp" />



    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Lokacija"
            android:layout_marginRight="33.0dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="35.0dp"
            android:layout_weight="0.50"
            android:layout_marginLeft="40dp"
            android:paddingTop="5dp"
            android:id="@+id/tbLocation" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Št. kopija"
            android:layout_marginRight="33.0dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="35.0dp"
            android:layout_weight="0.50"
            android:layout_marginLeft="34.5dp"
            android:paddingTop="5dp"
            android:id="@+id/tbNumberOfCopies" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Količina"
            android:layout_marginRight="33.5dp" />

        <EditText
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="44.5dp"
            android:layout_weight="0.50"
            android:layout_marginLeft="40dp"
            android:paddingTop="5dp"
            android:id="@+id/tbQty" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="58.5dp"
        android:orientation="horizontal"
        android:paddingTop="5dp">



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Subjekt"
            android:layout_marginRight="55.0dp" />
        <Spinner
            android:spinnerMode="dialog"
            android:layout_width="match_parent"
            android:layout_height="25.0dp"
            android:id="@+id/cbSubject"
            android:layout_marginTop="0.0dp" />


    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:paddingTop="5dp">

        <Button
            android:textSize="20dp"
            android:background="@drawable/nicelookingbutton"
            android:text="Tiskaj - f3"
            android:drawableLeft="@drawable/printer"
            android:paddingLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:layout_weight="1"
            android:id="@id/btPrint" />

        <Button
            android:textSize="20dp"
            android:background="@drawable/nicelookingbutton"
            android:text="Izhod - f8"
            android:drawableLeft="@drawable/logout"
            android:paddingLeft="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:id="@+id/button2" />


    </LinearLayout>


</LinearLayout>