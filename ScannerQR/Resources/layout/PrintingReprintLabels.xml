<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    tools:context=".LinearLayout"
    android:foregroundGravity="center">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="10px"
        android:paddingTop="5px"
        android:text="Tiskanje etiket - ponovno tiskanje" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Ident"
            android:layout_marginRight="62.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbIdent" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Naziv"
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:id="@+id/textView1"
            android:layout_marginRight="57.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbTitle" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="SSCC koda"
            android:layout_marginRight="20.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbSSCC" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
                                     >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Serijska št."
            android:paddingTop="5px"
            android:paddingLeft="10px"
            android:layout_marginRight="20.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40px"
            android:layout_weight="0.50"
            android:paddingTop="5px"
            android:id="@+id/tbSerialNum" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Skladišče"
            android:layout_marginRight="48.0dp" />

        <Spinner
            android:layout_width="match_parent"
            android:layout_height="42.5dp"
            android:id="@+id/cbWarehouse"
            android:layout_marginTop="24.5dp" />



    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Lokacija"
            android:layout_marginRight="33.0dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbLocation" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Količina"
            android:layout_marginRight="33.5dp" />

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbQty" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="57.0dp"
        android:orientation="horizontal"
        android:paddingTop="5px">



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10px"
            android:paddingTop="5px"
            android:text="Subjekt"
            android:layout_marginRight="55.0dp" />
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="25.0dp"
            android:id="@+id/cbSubject"
            android:layout_marginTop="24.5dp" />


    </LinearLayout>
    <Button
        android:text="Tiskaj - F3"
        android:layout_width="match_parent"
        android:layout_height="55.5dp"
        android:id="@+id/btPrint"
        android:layout_marginBottom="1.0dp" />
    <Button
        android:text="Izhod - F9"
        android:layout_width="match_parent"
        android:layout_height="58.0dp"
        android:id="@+id/button2" />


</LinearLayout>