<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:textStyle="bold"
        android:text="                          Inventura - Tiskanje"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="37.0dp"
        android:minWidth="25dp"
        android:minHeight="25dp"
        android:id="@+id/textView1sads"
        android:layout_marginLeft="0.0dp"
        android:layout_marginRight="0.0dp" />
    <TextView
        android:textStyle="bold"

        android:text="Skladišče"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="37.0dp"
        android:minWidth="25dp"
        android:minHeight="25dp"
        android:id="@+id/textView1" />
    <Spinner
        android:layout_width="match_parent"
        android:layout_height="42.0dp"
        android:spinnerMode="dialog"
        android:id="@+id/cbWarehouse" />
    <TextView
        android:textStyle="bold"
        android:text="Lokacija"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="37.0dp"
        android:minWidth="25dp"
        android:minHeight="25dp"
        android:id="@+id/textView1s" />
    <LinearLayout
        android:textStyle="bold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:minWidth="25dp"
        android:minHeight="25dp"
        android:id="@+id/linearLayout2" >

        <EditText
            android:textStyle="bold"
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40dp"
            android:paddingTop="5dp"
            android:id="@+id/tbLocation" />


    </LinearLayout>
 
    <LinearLayout
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:paddingTop="5dp">

        <Button
            android:textSize="20dp"
            android:background="@drawable/nicelookingbutton"
            android:text="Tiskaj - f2"
            android:drawableLeft="@drawable/printer"
            android:paddingLeft="10dp"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="70.0dp"
            android:id="@+id/btPrint" />

        <Button
            android:layout_weight="1"
            android:textSize="20dp"
            android:background="@drawable/nicelookingbutton"
            android:text="Izhod - f8"
            android:drawableLeft="@drawable/logout"
            android:paddingLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="70.0dp"
            android:id="@+id/button2" />


    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="302.0dp"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="302.0dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"/>


    </LinearLayout>
    
</LinearLayout>
