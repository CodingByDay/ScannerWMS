<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:text="                          Inventura - Tiskanje"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="37.0dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/textView1sads"
        android:layout_marginLeft="0.0dp"
        android:layout_marginRight="0.0dp" />
    <TextView
        android:text="Skladišče"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="37.0dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/textView1" />
    <Spinner
        android:layout_width="match_parent"
        android:layout_height="42.0dp"
        android:spinnerMode="dialog"
        android:id="@+id/cbWarehouse" />
    <TextView
        android:text="Lokacija"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="37.0dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/textView1s" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/linearLayout2" >

        <EditText
             android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:layout_marginLeft="40px"
            android:paddingTop="5px"
            android:id="@+id/tbLocation" />


    </LinearLayout>
    <Button
        android:text="Tiskaj - F2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btPrint" />
    <Button
        android:text="Izhod - F9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button2" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="302.0dp"
        android:paddingLeft="10px"
        android:paddingTop="5px"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="302.0dp"
            android:paddingLeft="10px"
            android:paddingTop="5px"/>


    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5px"
        android:minWidth="25px"
        android:minHeight="25px">


    </LinearLayout>
    <Button
        android:text="Potrdi - F3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button1" />
    <Button
        android:text="Izhod - F9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button2" />

</LinearLayout>
